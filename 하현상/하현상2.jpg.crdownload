<script type="text/javascript">
	var MaxCnt = 4;
	var topCnt = 1;
	function MoveTopEvent(type)
	{
		$("topEvent"+topCnt).style.display = "none";

		if(type =="next")
		{
			topCnt += 1;

			if(topCnt> MaxCnt )
			{
				topCnt = 1;
			}
			
			$("topEvent"+topCnt).style.display = "block";

		}
		else
		{
			topCnt -= 1;

			if(topCnt < 1)
			{
				topCnt = MaxCnt;
			}

			$("topEvent"+topCnt).style.display = "block";

		}
		
	}

